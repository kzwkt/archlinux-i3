
general {
        colors = true
        interval = 5
}
order += "volume master"
order += "wireless _first_"
order += "battery all"
order += "tztime local"

wireless _first_ {
        format_up = "W: (%quality ) %ip"
        format_down = "W: down"
}


battery all {
        format = "%status %percentage "
}

tztime local {
        format = "%C %b %A %I:%M:%S %p  "
}
volume master {
        format = "♪: %volume"
        format_muted = "♪: muted (%volume)"
}
